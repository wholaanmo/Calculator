    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        /*NEGATIVE SIGN (-) */
        jTextField1.setText(jTextField1.getText() + "-");
        double t = Double.parseDouble(jTextField1.getText()) * -1;
        jTextField1.setText(String.valueOf(t));
    }                                        

