    private void jButton39ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        /*SQRT*/
        num1 = Double.parseDouble(jTextField1.getText());
        if(num1<0){
        	jTextField1.setText("Error");
            return;
        }else{
        double t = num1;
        t=Math.sqrt(Double.parseDouble(jTextField1.getText()));
        jTextField1.setText(String.valueOf(t));
        }
    }                                         

    private void jButton38ActionPerformed(java.awt.event.ActionEvent evt) {                                          
         /*CBRT*/
        num1 = Double.parseDouble(jTextField1.getText());
        if(num1<0){
        	jTextField1.setText("Error");
            return;
        }else{
        double t = num1;
        t=Math.cbrt(Double.parseDouble(jTextField1.getText()));
        jTextField1.setText(String.valueOf(t));
        }
    }                                         

