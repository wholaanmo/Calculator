    private void jButton6ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        /* N! */
        double t=Double.parseDouble(jTextField1.getText());
        
        if(t % 1 != 0){
        	jTextField1.setText("Error");
            return;
        }
        else{
            double fact=1;
            while(t!=0){
            
               fact= fact*t;
               t--;
             }
             jTextField1.setText("");
             jTextField1.setText(jTextField1.getText() + fact);
        }
    }                                        

