    private void jButton35ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        /* EQUAL */
        num2 = Double.parseDouble(jTextField1.getText());
        
        switch (opr) {
        	case "+":
              result = num1 + num2;
              break;
              
            case "-":
              result = num1 - num2;
              break;
             
            case "*":
              result = num1 * num2;
              break;
             
            case "/":
              result = num1 / num2;
              break;
              
            case "%":
              result = num1 % num2;
              break;
              
            case "x^y":
              result = Math.pow(num1,num2);
              break;
              
           case "//":
              result = (int)(num1/num2);
              break;
              
           default:
              jTextField1.setText("Error: Unknown Operation");
              return;
        }       
        
        jTextField1.setText(Double.toString(result));      
    }
    
    private void jButton8ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        /* LOG2 X */
        double t=Math.log(Double.parseDouble(jTextField1.getText())) / Math.log(2);
        jTextField1.setText(String.valueOf(t));
    }                                        

